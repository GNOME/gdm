#: C/index.docbook:1966
#, no-wrap
msgid ""
"\n"
"[Desktop]\n"
"Session=gnome\n"
"Language=cs_CZ.UTF-8\n"
msgstr ""
"\n"
"[Desktop]\n"
"Session=gnome\n"
"Language=cs_CZ.UTF-8\n"

