#: daemon/gdm.c:1454
#, c-format
msgid "%s: Aborting display %s"
msgstr "%s: Se aborta la pantalla %s"

#: daemon/gdm.c:1592
msgid "GDM restarting ..."
msgstr "Reiniciando GDM..."

#: daemon/gdm.c:1596
