#: daemon/gdm.c:1179 daemon/gdm.c:2764
msgid "Master suspending..."
msgstr "Suspendiendo el maestro..."

#: daemon/gdm.c:1233
#, c-format
msgid "System is rebooting, please wait ..."
msgstr "El sistema se está reiniciando, espere por favor ..."

#: daemon/gdm.c:1235
#, c-format
msgid "System is shutting down, please wait ..."
msgstr "Es sistema se está deteniendo, espere por favor ..."

#: daemon/gdm.c:1246
msgid "Master halting..."
msgstr "Deteniendo al maestro..."

#: daemon/gdm.c:1259
#, c-format
msgid "%s: Halt failed: %s"
msgstr "%s: Ha fallado el apagado: %s"

#: daemon/gdm.c:1268
msgid "Master rebooting..."
msgstr "Reiniciando el maestro..."

#: daemon/gdm.c:1281
#, c-format
msgid "%s: Reboot failed: %s"
msgstr "%s: ha fallado el reinicio: %s"

