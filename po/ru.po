#: daemon/gdm.c:376
msgid "gdm_child_action: Master rebooting..."
msgstr "gdm_child_action: Системная перезагрузка..."

#: daemon/gdm.c:385
#, c-format
msgid "gdm_child_action: Reboot failed: %s"
msgstr "gdm_child_action: Не удалась перезагрузить систему: %s"

