#, c-format
msgid "initgroups () failed for %s"
msgstr "Dh’fhàillig le initgroups () airson %s"

#: ../daemon/gdm-server.c:406
#, c-format
#| msgid "Couldn't set userid to %d"
msgid "Couldn’t set userid to %d"
msgstr "Cha deach leinn userid a shuidheachadh air %d"

