msgid "gdm_config_parse: The gdm user should not be root. Aborting!"
msgstr "gdm_config_parse: Pengguna gdm tidak boleh root. Batal!"

#: daemon/gdm.c:515
#, c-format
msgid "gdm_config_parse: Can't find the gdm group (%s). Trying 'nobody'!"
msgstr "gdm_config_parse: Tiada kumpulan gdm (%s). Cuba 'nobody'!"

#: daemon/gdm.c:523
#, c-format
msgid "The gdm group does not exist. Please correct gdm configuration %s and restart gdm."
msgstr "kumpulan gdm tidak wujud. Sila betulkan konfigurasi gdm %s  dan ulanghidupkan gdm."

#: daemon/gdm.c:528
#, c-format
