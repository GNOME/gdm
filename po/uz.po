#: utils/gdm-config.c:1515 utils/gdmflexiserver.c:45
msgid "COMMAND"
msgstr "BUYRUQ"

#: utils/gdm-config.c:1519
#, c-format
msgid ""
"Commands:\n"
"  help         Shows this information\n"
"  password     Configure the password authentication\n"
"  fingerprint  Configure the fingerprint authentication\n"
"  smartcard    Configure the smartcard authentication\n"
"  reset        Resets the default configuration\n"
"  show         Shows the current configuration\n"
"\n"
"Use “%s COMMAND --help” to get help on each command.\n"
msgstr ""
"Buyruqlar:\n"
"  yordam         Ushbu maʼlumotni koʻrsatadi\n"
"  parol    Parol autentifikatsiyasini sozlang\n"
"  barmoq izi    Barmoq izi autentifikatsiyasini sozlang\n"
"  smartcard  Smart karta autentifikatsiyasini sozlang\n"
"  asliga qaytarish        Birlamchi konfiguratsiyani asliga qaytaradi\n"
