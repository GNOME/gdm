#, c-format
msgid "%s: Empty server command, using standard one."
msgstr "%s: Leerer Server-Befehl, Standard wird verwendet."

#: daemon/gdm.c:446
#, c-format
msgid "%s: Display number %d in use!  I will use %d"
msgstr "%s: Anzeige-Nummer %d in Gebrauch! %d wird verwendet."

#: daemon/gdm.c:463
