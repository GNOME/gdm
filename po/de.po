#, c-format
msgid "gdm_auth_user_add: Could not lock cookie file %s"
msgstr "gdm_auth_user_add: Cookie-Datei %s konnte nicht gesperrt werden."

#: daemon/auth.c:491
#, c-format
msgid "%s: Could not write cookie"
msgstr "%s: Cookie konnte nicht geschrieben werden"

